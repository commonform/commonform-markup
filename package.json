{
  "name": "commonform-markup",
  "description": "write form contracts in plain text",
  "version": "1.0.0-rc1",
  "author": {
    "name": "Kyle E. Mitchell",
    "email": "kyle@kemitchell.com",
    "url": "http://kemitchell.com/"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "check-versions": "1.x",
    "commonform-validate": "1.x",
    "istanbul": "^0.3.5",
    "jscs": "^1.8.1",
    "jshint": "^2.5.10",
    "mocha": "^2.0.1"
  },
  "homepage": "https://github.com/commonform/commonform-markup.js",
